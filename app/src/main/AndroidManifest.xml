<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sol.spotlight">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".RegisterResultActivity" />
        <activity android:name=".RegisterLaterActivity"></activity>
        <activity android:name=".RejectedActivity" />
        <activity android:name=".AcceptedActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RealMainActivity" />
        <activity android:name=".ChatRoomActivity" />
        <activity android:name=".ViewPagerPracticeActivity"></activity>
        <activity android:name=".IntroduceDetailActivity"></activity>
    </application>

</manifest>